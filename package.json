{
  "name": "mongo-airtable",
  "version": "2.0.0",
  "description": "Keep Airtable tables synced with local MongoDB collections",
  "author": "Phil Deschaine",
  "license": "MIT",
  "dependencies": {
    "airtable": "^0.10.1",
    "airtable-json": "^1.3.0",
    "core-js": "^3.8.1",
    "deep-equal": "^2.0.5",
    "dotenv": "^8.2.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "yaml": "^1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arro/mongo-airtable.git"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-plugin-babel": "^5.3.1",
    "mkdirp": "^1.0.4",
    "nock": "^13.0.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  },
  "ava": {
    "babel": true,
    "files": [
      "test/airtable.js",
      "test/mongodb.js"
    ],
    "@comment files": [],
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/run/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "test": "npx nyc ava",
    "build": "npx babel src --out-dir dist",
    "_pull": "node ./build/run/pull.js",
    "_push": "node ./build/run/push.js",
    "_update": "node ./build/run/update.js",
    "pull": "npm-run-all -s transpile _pull",
    "push": "npm-run-all -s transpile _push",
    "update": "npm-run-all -s transpile _update"
  }
}
