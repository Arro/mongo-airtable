{
  "name": "mongo-airtable",
  "version": "1.0.2",
  "description": "Keep Airtable tables synced with local MongoDB collections",
  "author": "Phil Deschaine",
  "license": "MIT",
  "dependencies": {
    "airtable": "^0.8.1",
    "airtable-json": "^1.2.5",
    "deep-equal": "^2.0.1",
    "mongodb": "^3.4.1",
    "nconf": "^0.10.0",
    "nconf-yaml": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "yamljs": "^0.2.8"
  },
  "devDependencies": {
    "@ava/babel": "^0.7.0",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.8.0",
    "@babel/runtime-corejs3": "^7.5.5",
    "ava": "^2.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "esm": "^3.2.25",
    "nock": "^11.7.2",
    "nyc": "^15.0.0"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false,
        "configFile": false,
        "presets": [
          [
            "@babel/preset-env",
            {
              "useBuiltIns": "entry",
              "corejs": 3
            }
          ]
        ],
        "plugins": [
          [
            "@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
          ]
        ]
      }
    },
    "files": [
      "test/main.js"
    ],
    "require": [
      "esm"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/run/*.js"
    ]
  },
  "engines": {
    "node": ">=13.5.0"
  },
  "scripts": {
    "test": "npx nyc ava",
    "transpile": "npx babel src --out-dir build",
    "initial-pull": "node ./build/run/initial-pull.js",
    "create-new": "node ./build/run/create-new.js",
    "push-changed": "node ./build/run/push-changed.js",
    "initial-insert": "node ./build/run/initial-insert.js",
    "look-for-changes": "node ./build/run/look-for-changes.js",
    "look-for-new-items": "node ./build/run/look-for-new-items.js",
    "pull": "npm-run-all -s transpile initial-pull initial-insert",
    "push": "npm-run-all -s transpile look-for-changes look-for-new-items create-new push-changed"
  }
}
